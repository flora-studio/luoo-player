{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Luoo",
  "version": "1.0.0",
  "identifier": "app.florastudio.luoo",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devUrl": "https://www.indie.cn",
    "frontendDist": "https://www.indie.cn"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "雀乐",
        "width": 1280,
        "height": 720,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["nsis", "dmg"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
